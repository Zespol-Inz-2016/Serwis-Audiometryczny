@model IEnumerable<SerwisAudiometryczny.Models.FrequencyModel>

@{
    ViewBag.Title = "Nowa częstotliwość";
}

<h2>Nowa częstotliwość</h2>

<p>
        @Html.ActionLink("Stwórz nowy", "Create", routeValues: null, htmlAttributes: new { Class = "btn btn-info" })
    @if (ViewBag.GenerateButton)
    {
        @Html.ActionLink("Generuj 11 oktaw", "GenerateOctave", routeValues: null, htmlAttributes: new { Class = "btn btn-info" })
    }
</p>
<table class="table f-table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Frequency)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Frequency)
        </td>
        <td>
            @Html.ActionLink("Edytuj", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Szczegóły", "Details", new { id=item.ID }) 
        </td>
    </tr>
}

</table>
